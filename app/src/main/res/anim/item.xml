<?xml version="1.0" encoding="utf-8"?>
<set xmlns:android="http://schemas.android.com/apk/res/android"
    android:duration="300"
    android:shareInterpolator="true">
    //每个item 淡入
    <alpha
        android:fromAlpha="0.0"
        android:toAlpha="1.0" />

    //每个item移动距离
    <translate
        android:fromXDelta="100"
        android:toXDelta="0" />

</set>